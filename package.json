    "prompts": "^2.4.1"
  },
  "devDependencies": {
    "@chenyueban/lint": "^1.1.0",
    "@chenyueban/tsconfig": "^0.2.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.1",
    "@types/prompts": "^2.0.14",
    "@types/rimraf": "^3.0.2",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "express": "^4.17.1",
    "jest": "^27.1.1",
    "multer": "^1.4.3",
    "release-it": "^14.11.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
